public class Main {
    public static void main(String args[]){
        sayHello();       //method calling without parameters: 
      
        sayHello("Hello JAVA:");    //method calling with parameters:

    }

public static void sayHello(){   //creating a method without parameter: 
    System.out.println("HELLO GHF FAMILY: ");
    
}

public static void sayHello(String a){ //creating a method with parameter:
     
    System.out.println(a);

}
}
